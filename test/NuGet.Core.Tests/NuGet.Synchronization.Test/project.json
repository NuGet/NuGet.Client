{
  "version": "4.0.1-*",
  "buildOptions": {
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },
  "dependencies": {
    "NuGet.Test.Utility": {
      "target": "project"
    },
    "SynchronizationTestApp": {
      "target": "project"
    },
    "xunit": "2.1.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "netstandardapp1.5",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "dotnet-test-xunit": "1.0.0-rc2-173384-19",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "Microsoft.CSharp": "",
        "System": "",
        "System.Core": "",
        "System.Runtime": "",
        "System.Threading.Tasks": ""
      },
      "buildOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    }
  },
  "testRunner": "xunit"
}
