{
  "version": "4.0.1-*",
  "authors": [
    "NuGet"
  ],
  "description": "NuGet's protocol-level base types used for connecting to API v2 and API v3 repositories.",
  "copyright": "Copyright .NET Foundation. All rights reserved.",
  "packOptions": {
    "tags": [
      "nuget protocol"
    ],
    "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt",
    "projectUrl": "https://github.com/NuGet/NuGet.Client"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591",
      "CS1573"
    ],
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },
  "dependencies": {
    "NuGet.Common": {
      "target": "project"
    },
    "NuGet.Configuration": {
      "target": "project"
    },
    "NuGet.Packaging": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": ""
      },
      "buildOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Net.Http": "4.1.0"
      },
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    }
  }
}
