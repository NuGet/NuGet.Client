{
  "version": "4.0.1-*",
  "authors": [
    "NuGet"
  ],
  "description": "NuGet's client configuration settings implementation.",
  "copyright": "Copyright .NET Foundation. All rights reserved.",
  "packOptions": {
    "tags": [
      "nuget",
      "configuration",
      "nuget.config"
    ],
    "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt",
    "projectUrl": "https://github.com/NuGet/NuGet.Client"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ],
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },
  "dependencies": {
    "NuGet.Common": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Security": "",
        "System.Xml": "",
        "System.Xml.Linq": ""
      },
      "buildOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Security.Cryptography.ProtectedData": "4.0.0",
        "System.Xml.XDocument": "4.0.11"
      },
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    }
  }
}
