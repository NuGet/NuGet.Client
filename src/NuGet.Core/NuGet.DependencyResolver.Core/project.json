{
  "version": "4.0.1-*",
  "description": "NuGet v3 core library.",
  "copyright": "Copyright .NET Foundation. All rights reserved.",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt",
    "projectUrl": "https://github.com/NuGet/NuGet.Client"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591",
      "CS1574"
    ],
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },
  "dependencies": {
    "NuGet.LibraryModel": {
      "target": "project"
    },
    "NuGet.Frameworks": {
      "target": "project"
    },
    "NuGet.Protocol.Core.v3": {
      "target": "project"
    },
    "NuGet.Repositories": {
      "target": "project"
    },
    "NuGet.RuntimeModel": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      },
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    }
  }
}
