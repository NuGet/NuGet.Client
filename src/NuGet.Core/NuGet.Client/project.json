{
  "version": "3.5.0-*",
  "description": "NuGet v3 core library.",
  "copyright": "Copyright .NET Foundation. All rights reserved.",
  "packOptions": {
    "projectUrl": "https://github.com/NuGet/NuGet.Client",
    "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ],
    "compile": [
      "../../../Shared/*.cs"
    ]
  },
  "dependencies": {
    "NuGet.Versioning": {
      "target": "project"
    },
    "NuGet.Packaging": {
      "target": "project"
    },
    "NuGet.Repositories": {
      "target": "project"
    },
    "NuGet.RuntimeModel": {
      "target": "project"
    },
    "NuGet.ContentModel": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    },
    "netstandard1.3": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      },
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    }
  }
}