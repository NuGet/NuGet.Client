{
  "version": "3.5.0-*",
  "description": "The core data structures for NuGet.Packaging",
  "authors": [
    "NuGet"
  ],
  "copyright": "Copyright .NET Foundation. All rights reserved.",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt",
    "projectUrl": "https://github.com/NuGet/NuGet.Client"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ],
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },
  "dependencies": {
    "NuGet.Common": {
      "target": "project"
    },
    "NuGet.Packaging.Core.Types": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "frameworkAssemblies": {
        "System.Xml": "",
        "System.Xml.Linq": "",
        "System.IO.Compression": ""
      },
      "buildOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0-rc3-24128-00",
        "System.Xml.XDocument": "4.0.11-rc3-24128-00"
      },
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    }
  }
}