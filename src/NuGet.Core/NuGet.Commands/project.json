  "projectUrl": "https://github.com/NuGet/NuGet.Client",
  "licenseUrl": "https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/LICENSE.txt",
  "description": "Complete commands common to command-line and GUI NuGet clients",
  "compilationOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591",
      "CS1574",
      "CS1573",
      "CS1584",
      "CS1658"
    ]
  },
  "compile": [
    "../NuGet.Shared/*.cs",
    "../../../submodules/FileSystem/src/Microsoft.Extensions.FileSystemGlobbing/**/*.cs",
    "../../../submodules/FileSystem/src/Microsoft.AspNet.FileProviders.Abstractions/**/*.cs",
    "../../../submodules/FileSystem/src/Microsoft.AspNet.FileProviders.Sources/**/*.cs",
    "../../../submodules/Common/src/Microsoft.Extensions.Primitives/IChangeToken.cs"
  ],
  "compileExclude": [
    "../../../submodules/FileSystem/src/**/AssemblyInfo.cs"
  ],
  "dependencies": {
    "NuGet.Client": {
      "target": "project"
    },
    "NuGet.ContentModel": {
      "target": "project"
    },
    "NuGet.ProjectModel": {
      "target": "project"
    },
    "NuGet.Configuration": {
      "target": "project"
    },
    "NuGet.DependencyResolver": {
      "target": "project"
    },
    "NuGet.RuntimeModel": {
      "target": "project"
    }
  },
  "frameworks": {
    "net45": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "frameworkAssemblies": {
        "System.Xml": "",
        "System.Xml.Linq": ""
      },
      "compilationOptions": {
        "define": [
          "IS_DESKTOP"
        ]
      }
    },
    "netstandard1.3": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027",
        "System.Xml.XDocument": "4.0.11-rc2-24027"
      },
      "compilationOptions": {
        "define": [
          "IS_CORECLR"
        ]
      }
    }
  }
}