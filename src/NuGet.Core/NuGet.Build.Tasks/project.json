{
  "version": "4.0.1-*",
  "description": "NuGet 3 restore for dotnet CLI",
  "packOptions": {
    "files": {
      "mappings": {
        "runtimes/any/native/NuGet.targets": "NuGet.targets"
      }
    }
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true,
    "nowarn": [
      "CS1591"
    ],
    "compile": {
      "include": [
        "../../../Shared/*.cs"
      ]
    }
  },

  "dependencies": {
    "NuGet.Commands": {
      "target": "project"
    }
  },

  "frameworks": {
    "net45": {
      "define": [
        "IS_DESKTOP"
      ],
      "dependencies": {
      },
      "frameworkAssemblies": {
        "Microsoft.Build.Utilities.v4.0": {
          "type": "build"
        },
        "Microsoft.Build.Framework": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [
          "IS_CORECLR"
        ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "Microsoft.Build.Framework": "0.1.0-preview-00038-160914",
        "Microsoft.Build.Tasks.Core": "0.1.0-preview-00038-160914",
        "Microsoft.Build.Utilities.Core": "0.1.0-preview-00038-160914"
      }
    }
  }
}
