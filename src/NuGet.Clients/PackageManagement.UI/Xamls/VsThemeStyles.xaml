<ResourceDictionary x:Class="NuGet.PackageManagement.UI.VsThemeStyles"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:NuGet.PackageManagement.UI"
                    xmlns:vs="clr-namespace:Microsoft.VisualStudio.Shell">
  <Style x:Key="UITextForeground" TargetType="{x:Type TextBlock}">
    <Setter Property="Foreground" Value="1{DynamicResource {x:Static vs:VsBrushes.BrandedUITextKey}}"/>
  </Style>
</ResourceDictionary>
