   <Target Name="AfterBuild" DependsOnTargets="PublishVS14;PublishVS15;PublishVS15Insertable" />
   <Target Name="PublishVS14" Condition="'$(VisualStudioVersion)' == '14.0'">
      <PropertyGroup>
         <PublishDestination>$(ArtifactRoot)\$(VsixOutputDirName)</PublishDestination>
      </PropertyGroup>
      <Message Text="Publishing VS14 VSIX package files..." Importance="high" />
      <Message Text="$(TargetVsixContainer) -&amp;gt; $(PublishDestination)" Importance="high" />
      <Copy SourceFiles="$(TargetVsixContainer)" DestinationFolder="$(PublishDestination)" OverwriteReadOnlyFiles="true" />
      <ItemGroup>
         <NuGetSymbolFiles Include="$(OutputPath)NuGet*.pdb" />
      </ItemGroup>
      <Message Text="Copying symbols file '%(NuGetSymbolFiles.Identity)'" Condition=" '@(NuGetSymbolFiles)' != '' " />
      <Copy SourceFiles="@(NuGetSymbolFiles)" DestinationFolder="$(PublishDestination)\symbols" OverwriteReadOnlyFiles="true" />
   </Target>
   <Target Name="PublishVS15" Condition="'$(IsInsertable)' != 'true' AND '$(VisualStudioVersion)' == '15.0'">
      <PropertyGroup>
         <PublishDestination>$(ArtifactRoot)\$(VsixOutputDirName)</PublishDestination>
      </PropertyGroup>
      <ItemGroup>
         <PackageManifest Include="$(OutputPath)Microsoft.VisualStudio.NuGet.Core.json" />
      </ItemGroup>
      <Message Text="Publishing VS15 VSIX package files..." Importance="high" />
      <Message Text="$(TargetVsixContainer) -&amp;gt; $(PublishDestination)" Importance="high" />
      <Copy SourceFiles="$(TargetVsixContainer)" DestinationFolder="$(PublishDestination)" OverwriteReadOnlyFiles="true" />
      <Message Text="@(PackageManifest) -&amp;gt; $(PublishDestination)" Importance="high" />
      <Copy SourceFiles="@(PackageManifest)" DestinationFolder="$(PublishDestination)" OverwriteReadOnlyFiles="true" />
      <ItemGroup>
         <NuGetSymbolFiles Include="$(OutputPath)NuGet*.pdb" />
      </ItemGroup>
      <Message Text="Copying symbols file '%(NuGetSymbolFiles.Identity)'" Condition=" '@(NuGetSymbolFiles)' != '' " />
      <Copy SourceFiles="@(NuGetSymbolFiles)" DestinationFolder="$(PublishDestination)\symbols" OverwriteReadOnlyFiles="true" />
   </Target>
   <Target Name="PublishVS15Insertable" Condition="'$(IsInsertable)' == 'true' AND '$(VisualStudioVersion)' == '15.0'">
      <PropertyGroup>
         <PublishDestination>$(ArtifactRoot)\$(VsixOutputDirName)</PublishDestination>
      </PropertyGroup>
      <ItemGroup>
         <PackageManifest Include="$(OutputPath)Microsoft.VisualStudio.NuGet.Core.json" />
      </ItemGroup>
      <Message Text="Publishing VS15 Insertable VSIX package files..." Importance="high" />
      <Message Text="$(TargetVsixContainer) -&amp;gt; $(PublishDestination)\Insertable" Importance="high" />
      <Copy SourceFiles="$(TargetVsixContainer)" DestinationFolder="$(PublishDestination)\Insertable" OverwriteReadOnlyFiles="true" />
      <Message Text="@(PackageManifest) -&amp;gt; $(PublishDestination)" Importance="high" />
      <Copy SourceFiles="@(PackageManifest)" DestinationFolder="$(PublishDestination)" OverwriteReadOnlyFiles="true" />
      <ItemGroup>
         <NuGetSymbolFiles Include="$(OutputPath)NuGet*.pdb" />
      </ItemGroup>
      <Message Text="Copying symbols file '%(NuGetSymbolFiles.Identity)'" Condition=" '@(NuGetSymbolFiles)' != '' " />
      <Copy SourceFiles="@(NuGetSymbolFiles)" DestinationFolder="$(PublishDestination)\symbols" OverwriteReadOnlyFiles="true" />
   </Target>
   <Target Name="ReadVsixIncludeListFromFile">
      <ReadLinesFromFile File="@(VsixIncludeFile)">
         <Output TaskParameter="Lines" ItemName="IncludeVsixFilesList" />
      </ReadLinesFromFile>
      <ItemGroup>
         <IncludeVsixFilesList Remove="@(IncludeVsixFilesList)" Condition="'%(Identity)' != '' AND $([System.String]::new('%(Identity)').StartsWith('#'))" />
      </ItemGroup>
   </Target>
   <Target Name="ReadVsixIgnoreListFromFile">
      <ReadLinesFromFile File="@(VsixIgnoreFile)">
         <Output TaskParameter="Lines" ItemName="IgnoreVsixFilesList" />
      </ReadLinesFromFile>
      <ItemGroup>
         <IgnoreVsixFilesList Remove="@(IgnoreVsixFilesList)" Condition="'%(Identity)' != '' AND $([System.String]::new('%(Identity)').StartsWith('#'))" />
      </ItemGroup>
   </Target>
   <Target Name="RemoveUnsolicitedVsixAssemblies" AfterTargets="GetVsixSourceItems" Condition="'$(IncludeCopyLocalReferencesInVSIXContainer)'=='true'" DependsOnTargets="ReadVsixIncludeListFromFile;ReadVsixIgnoreListFromFile">
      <Message Text="Filtering VSIX package files..." Importance="high" />
      <ItemGroup>
         <AllVsixAssemblies Include="@(VSIXSourceItem-&gt;'%(FileName)%(Extension)')" Condition=" '%(Extension)' == '.dll' ">
            <SourceFile>%(VSIXSourceItem.Identity)</SourceFile>
         </AllVsixAssemblies>
      </ItemGroup>
      <ItemGroup>
         <SuppressPackaging Include="@(AllVsixAssemblies)" Exclude="@(IncludeVsixFilesList)" />
         <MissingRequiredAssemblies Include="@(IncludeVsixFilesList)" Exclude="@(AllVsixAssemblies)" />
         <NotListedDependencies Include="@(SuppressPackaging)" Exclude="@(IgnoreVsixFilesList)" />
         <MissingIgnoredAssemblies Include="@(IgnoreVsixFilesList)" Exclude="@(AllVsixAssemblies)" />
         <VSIXSourceItem Remove="@(SuppressPackaging-&gt;'%(SourceFile)')" />
      </ItemGroup>
      <Message Text="Suppressed file '%(SuppressPackaging.SourceFile)'" Condition=" '@(SuppressPackaging)' != '' " />
      <Message Text="Included file '%(VSIXSourceItem.Identity)'" Condition=" '@(VSIXSourceItem)' != '' " />
      <Error Text="Missing @(MissingRequiredAssemblies-&amp;gt;Count()) required file(s) '@(MissingRequiredAssemblies)'" Condition=" '@(MissingRequiredAssemblies)' != '' " File="@(VsixIncludeFile)" />
      <Error Text="Found @(NotListedDependencies-&amp;gt;Count()) not listed file(s) '@(NotListedDependencies)'" Condition=" '@(NotListedDependencies)' != '' " File="@(VsixIncludeFile)" />
      <Warning Text="Missing @(MissingIgnoredAssemblies-&amp;gt;Count()) ignored file(s) '@(MissingIgnoredAssemblies)'" Condition=" '@(MissingIgnoredAssemblies)' != '' " File="@(VsixIgnoreFile)" />
      <Message Text="Included @(VSIXSourceItem-&amp;gt;Count()) file(s). Filtered @(SuppressPackaging-&amp;gt;Count()) file(s)." Importance="high" />
   </Target>
</Project>